{
    "name": "efabrica/nette-repository",
    "type": "library",
    "description": "Repositories for Nette database",
    "keywords": [
        "php",
        "nette",
        "database",
        "repository"
    ],
    "readme": "README.md",
    "license": "MIT",
    "authors": [
        {
            "name": "Richard Popelis",
            "email": "richard@popelis.sk"
        },
        {
            "name": "Martin Petricko",
            "email": "matopetricko@gmail.com"
        },
        {
            "name": "Samuel Kelemen",
            "email": "kelemen.samuel@gmail.com"
        },
        {
            "name": "Michal Lulco",
            "email": "michal.lulco@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-pdo": "*",
        "doctrine/inflector": "^2.0",
        "nette/caching": "^3.1",
        "nette/database": "^3.2",
        "nette/di": "^3.1",
        "nette/php-generator": "^4.1.4",
        "nikic/php-parser": "^4.16|^5.0",
        "symfony/console": "^5.4|^6.0|^7.0",
        "symfony/polyfill-php80": "^1.29"
    },
    "require-dev": {
        "nette/bootstrap": "^3.1",
        "tracy/tracy": "^2.9",
        "nette/http": "^3.2",
        "phpunit/phpunit": "^10.0",
        "nette/application": "^3.1",
        "nesbot/carbon": "^3.1",
        "ramsey/uuid": "^4.2"
    },
    "autoload": {
        "psr-4": {
            "Efabrica\\NetteRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "bin": [
        "bin/enc"
    ]
}
